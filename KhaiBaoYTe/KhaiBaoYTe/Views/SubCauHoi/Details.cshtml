@model KhaiBaoYTe.Models.Sub_CauHoi

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="text-center">
    <h2>Details</h2>
    <h4>Sub_CauHoi</h4>
</div>
<hr />
<div class="row justify-content-around">
    <div class="col-md-7">
        <dl class="row">
            <dt class="col-6">
                Tiêu đề
            </dt>

            <dd class="col-6">
                @Html.DisplayFor(model => model.CauHoi.TieuDe)
            </dd>

            <dt class="col-6">
                Nội dung
            </dt>

            <dd class="col-6">
                @Html.DisplayFor(model => model.NoiDung)
            </dd>
        </dl>
    </div>
</div>
<div class="text-center mt-md-4">
    <a href="@Url.Action("Index", "SubCauHoi", new { stringId = Request.QueryString["stringId"] })" class="btn btn-default">
        Back to Nội dung
    </a>
</div>
